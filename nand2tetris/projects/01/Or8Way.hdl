/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=outAux);
    Or(a=outAux, b=in[2], out=outAux);
    Or(a=outAux, b=in[3], out=outAux);
    Or(a=outAux, b=in[4], out=outAux);
    Or(a=outAux, b=in[5], out=outAux);
    Or(a=outAux, b=in[6], out=outAux);
    Or(a=outAux, b=in[7], out=out);
}
