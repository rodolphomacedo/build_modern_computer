/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(a=a, out=nota); 
    Not(b=b, out=notb);
    And(a=a, b=notb, out=aEnotb);
    And(a=nota, b=b, out=notaEb);
    Or(a=aEnotb, b=notaEb, out=out);
}
